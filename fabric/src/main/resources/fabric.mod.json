{
  "schemaVersion": 1,
  "id": "opc",
  "version": "${version}",
  "name": "Offline Player Cache",
  "description": "Allows the ability to cache player data to the server based on cached keys.",
  "authors": [
    "Bare Minimum Studios",
    "CleverNucleus (former author)"
  ],
  "contact": {
    "homepage": "https://bareminimumstudios.github.io/Bare-Minimum-Docs/",
    "sources": "https://github.com/BareMinimumStudios/Offline-Player-Cache"
  },
  "license": "MIT",
  "icon": "assets/opc/opc_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.bibireden.opc.fabric.OfflinePlayerCacheInitializerFabric"
    ]
  },
  "mixins": [
    "opc.mixins.json",
    "opc-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": "~1.20.1",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.10.19+kotlin.1.9.23"
  }
}