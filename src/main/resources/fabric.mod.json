{
	"schemaVersion": 1,
	"id": "offline-player-cache",
	"version": "${version}",
	"name": "Offline Player Cache: Directors Cut",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"bibi-reden",
		"DataEncoded",
		"pokesmells",
		"clevernucleus (original author)"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/offline-player-cache/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.bibireden.opc.OfflinePlayerCacheServer",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.bibireden.opc.OfflinePlayerCacheClient",
				"adapter": "kotlin"
			}
		],
		"cardinal-components": [
			"com.bibireden.opc.ComponentInitializer"
		]
	},
	"mixins": [
		"offline-player-cache.mixins.json",
		{
			"config": "offline-player-cache.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.24"
	},
	"custom": {
		"cardinal-components": [
			"offline-player-cache:values"
		]
	}
}